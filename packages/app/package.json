{
  "name": "@himenon/microfrontend-app",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/microfrontend.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production yarn webpack:build",
    "webpack:build": "micrforntend-builder --build --app --external-assets '@himenon/microfrontend-components:MicroComponent,@himenon/microfrontend-tutorial:Tutorial'",    
    "clean": "rimraf index.d.ts docs coverage module umd dist *.tsbuildinfo",
    "server": "cross-env NODE_ENV=development micrforntend-builder --server",
    "test": "jest -c jest.config.js"
  },
  "dependencies": {
    "@himenon/microfrontend-components": "0.1.0",
    "@himenon/microfrontend-tutorial": "0.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@himenon/microfrontend-builder": "0.1.0"
  }
}
