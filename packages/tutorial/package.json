{
  "name": "@himenon/microfrontend-tutorial",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/microfrontend.git"
  },
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=development yarn webpack:build",
    "build:dev": "cross-env NODE_ENV=development micrforntend-builder --build",
    "server": "cross-env NODE_ENV=development yarn webpack:server",
    "test": "jest -c jest.config.js",
    "webpack:build": "micrforntend-builder --build --library Tutorial --external-assets '@himenon/microfrontend-components:MicroComponent'",
    "webpack:server": "micrforntend-builder --dev-server --library Tutorial --external-assets '@himenon/microfrontend-components:MicroComponent'"
  },
  "dependencies": {
    "@himenon/microfrontend-components": "0.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@himenon/microfrontend-builder": "0.1.0",
    "tslib": "^2.0.0"
  }
}
