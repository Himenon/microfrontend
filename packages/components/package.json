{
  "name": "@himenon/microfrontend-components",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/microfrontend.git"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dist"
  ],
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production yarn webpack:build && yarn tsc:build && yarn dts:build",
    "build:dev": "cross-env NODE_ENV=development micrforntend-builder --build",
    "clean": "rimraf index.d.ts docs coverage module umd dist *.tsbuildinfo",
    "dts:build": "micrforntend-builder --dts-bundle",
    "server": "cross-env NODE_ENV=development yarn webpack:server",
    "test": "jest -c jest.config.js",
    "tsc:build": "tsc -b tsconfig.module.json",
    "webpack:build": "micrforntend-builder --build --library MicroComponent",
    "webpack:server": "micrforntend-builder --dev-server --library MicroComponent"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@himenon/microfrontend-builder": "0.1.0"
  }
}
