{
  "name": "@himenon/microfrontend-components",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/microfrontend.git"
  },
  "license": "MIT",
  "main": "./umd/MicroComponent.js",
  "module": "./module/index.js",
  "types": "./index.d.ts",
  "scripts": {
    "build": "cross-env NODE_ENV=production micrforntend-builder --build --library MicroComponent && yarn tsc:build && yarn dts:build",
    "build:dev": "cross-env NODE_ENV=development micrforntend-builder --build",
    "clean": "rimraf module umd dist *.tsbuildinfo",
    "dts:build": "micrforntend-builder --dts-bundle",
    "server": "cross-env NODE_ENV=development micrforntend-builder --server",
    "test": "jest -c jest.config.js",
    "tsc:build": "tsc -b tsconfig.module.json"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@himenon/microfrontend-builder": "0.1.0"
  }
}
